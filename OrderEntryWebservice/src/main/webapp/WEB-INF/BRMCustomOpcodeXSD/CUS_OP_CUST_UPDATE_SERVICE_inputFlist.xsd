        <xs:element ref="brm:CUS_FLD_DEVICE_MODEL"/>
        <xs:element ref="brm:CUS_FLD_DEVICE_TYPE"/>
        <xs:element ref="brm:REASON_CODE"/>
        <xs:element ref="brm:CUS_FLD_INSTALLMENT_PLAN"/>
        <xs:element ref="brm:USER_NAME"/>
        <xs:element ref="brm:SERVICE_ID"/>
        <xs:element maxOccurs="unbounded" ref="brm:PLAN"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="POID" type="xs:string"/>
  <xs:element name="ACTION" type="xs:string"/>
