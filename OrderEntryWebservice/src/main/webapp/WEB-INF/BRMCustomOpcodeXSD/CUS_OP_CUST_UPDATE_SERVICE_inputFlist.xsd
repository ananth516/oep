  <xs:element name="SERVICE_ID" type="xs:string"/>
  <xs:element name="PLAN">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="brm:NAME"/>
        <xs:element ref="brm:CUS_FLD_PLAN_TYPE"/>
      </xs:sequence>
      <xs:attribute name="elem" use="required" type="xs:int"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="NAME" type="xs:string"/>
  <xs:element name="CUS_FLD_PLAN_TYPE" type="xs:int"/>
</xs:schema>
