<?xml version = '1.0' encoding = 'UTF-8'?>
<xsd:schema elementFormDefault="qualified"
	targetNamespace="http://xmlns.oracle.com/communications/schemas/CRMSync"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://xmlns.oracle.com/communications/schemas/CRMSync">
	<xsd:element name="SpendingCapAction">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AccountNo" minOccurs="1" maxOccurs="1" />
				<xsd:element ref="BarringAction" minOccurs="0"
					maxOccurs="1" />
				<xsd:element ref="BarringLevel" minOccurs="0" maxOccurs="1" /> 	
				<xsd:element ref="ServiceIdentifier" minOccurs="0"
					maxOccurs="1" />
				<xsd:element ref="MSISDN" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="PlanName" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="BarringType" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="ActionCode" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="SpendingNotifyContacts" minOccurs="0"
					maxOccurs="unbounded" />
			</xsd:sequence>
			<xsd:attribute name="InstanceId" type="xsd:string" use="required" />
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SpendingNotifyContacts">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="EmailAddress" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="Phone" minOccurs="0" maxOccurs="1" />
			</xsd:sequence>
			<xsd:attribute name="ElementId" type="xsd:string" use="required" />
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AccountNo" type="xsd:string" />
	<xsd:element name="BarringAction" type="xsd:string" />
	<xsd:element name="BarringLevel" type="xsd:string" />
	<xsd:element name="ServiceIdentifier" type="xsd:string" />
	<xsd:element name="MSISDN" type="xsd:string" />
	<xsd:element name="PlanName" type="xsd:string" />
	<xsd:element name="BarringType" type="xsd:string" />
	<xsd:element name="ActionCode" type="xsd:string" />
	<xsd:element name="EmailAddress" type="xsd:string" />
	<xsd:element name="Phone" type="xsd:string" />
</xsd:schema>